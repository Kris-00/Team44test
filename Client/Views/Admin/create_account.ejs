<!DOCTYPE html>
<html lang="en">


<%- include('../partials/_head.ejs') %>

    <body>
        <!--start wrapper-->
        <div class="wrapper">
            <!--start top header-->
            <%- include('../partials/_navbar.ejs') %>
                <!--end top header-->

                <!--start sidebar -->
                <%- include('../partials/_sidebar.ejs') %>
                    <!--end sidebar -->

                    <!--start content-->
                    <main class="page-content">

                        <!--breadcrumb-->
                        <div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
                            <div class="breadcrumb-title pe-3">Accounts</div>
                            <div class="ps-3">
                                <nav aria-label="breadcrumb">
                                    <ol class="breadcrumb mb-0 p-0">
                                        <li class="breadcrumb-item">
                                            <a href="javascript:;"><i class="bx bx-home-alt"></i></a>
                                        </li>
                                        <li class="breadcrumb-item active" aria-current="page">
                                            Create Account
                                        </li>
                                    </ol>
                                </nav>
                            </div>
                        </div>
                        <!--end breadcrumb-->

                        <div class="card shadow-sm border-0">
                            <div class="card-body">
                                <h5 class="mb-0">New Admin Account</h5>
                                <hr>
                                <div class="card shadow-none border">
                                    <div class="card-header">
                                        <h6 class="mb-0">USER INFORMATION</h6>
                                    </div>
                                    <span class="text-danger" id="errMsg"></span>
                                    <div class="card-body">
                                        <form class="row g-3">
                                            <div class="col-6">
                                                <label class="form-label">Email address</label>
                                                <input id="email" type="text" class="form-control" value="">
                                            </div>
                                            <div class="col-6">
                                                <label class="form-label">Phone</label>
                                                <input id="phone" maxlength="8" type="number" class="form-control"
                                                    value="">
                                            </div>
                                            <div class="col-6">
                                                <label class="form-label">First Name</label>
                                                <input id="fName" type="text" class="form-control" value="">
                                            </div>
                                            <div class="col-6">
                                                <label class="form-label">Last Name</label>
                                                <input id="lName" type="text" class="form-control" value="">
                                            </div>
                                            <div class="col-6">
                                                <label class="form-label">Password</label>
                                                <input id="password" type="password" class="form-control" value="">
                                            </div>
                                            <div class="col-6">
                                                <label class="form-label">Confirm Password</label>
                                                <input id="cfmPassword" type="password" class="form-control" value="">
                                            </div>
                                            <div class="col-6">
                                                <div class="form-check">
                                                    <input class="form-check-input" type="checkbox" name="select"
                                                        id="activate">
                                                    <label class="form-check-label" for="activate">
                                                        Acitivate account
                                                    </label>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                                <div class="text-start">
                                    <button id="crtAcc" type="button" class="btn btn-primary px-4">Create New
                                        Account</button>
                                </div>
                            </div>
                        </div>


                    </main>
                    <!--end page main-->

                    <!--start overlay-->
                    <div class="overlay nav-toggle-icon"></div>
                    <!--end overlay-->

                    <!--Start Back To Top Button-->
                    <a href="javaScript:;" class="back-to-top"><i class="bx bxs-up-arrow-alt"></i></a>
                    <!--End Back To Top Button-->

        </div>
        <!--end wrapper-->

        <!-- Bootstrap bundle JS -->
        <script src="../../assets/js/bootstrap.bundle.min.js"></script>
        <!--plugins-->
        <script src="../../assets/js/jquery.min.js"></script>
        <script src="../../assets/plugins/simplebar/js/simplebar.min.js"></script>
        <script src="../../assets/plugins/metismenu/js/metisMenu.min.js"></script>
        <script src="../../assets/plugins/perfect-scrollbar/js/perfect-scrollbar.js"></script>
        <script src="../../assets/js/pace.min.js"></script>
        <!--app-->
        <script src="../../assets/js/app.js"></script>
        <script type="module" src="../../assets/js/module/users/create_admin.js"></script>
    </body>

</html>